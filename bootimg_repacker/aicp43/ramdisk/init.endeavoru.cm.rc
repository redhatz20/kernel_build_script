on early-init
    chown system /sys/kernel/tegra_cap/core_cap_level
    chown system /sys/kernel/tegra_cap/core_cap_state
    chown system /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor

# Settings Tegra3Parts
    chown system system /sys/devices/platform/htc_battery/fast_charge
    chmod 0666 /sys/devices/platform/htc_battery/fast_charge
    chown system system /sys/android_touch/calibration_control
    chmod 0666 /sys/android_touch/calibration_control
    chown system system /sys/android_touch/s2w_allow_double_tap
    chmod 0666 /sys/android_touch/s2w_allow_double_tap
    chown system system /sys/android_touch/s2w_allow_stroke
    chmod 0666 /sys/android_touch/s2w_allow_stroke
    chown system system /sys/android_touch/s2w_double_tap_barrier_y
    chmod 0666 /sys/android_touch/s2w_double_tap_barrier_y
    chown system system /sys/android_touch/s2w_double_tap_duration
    chmod 0666 /sys/android_touch/s2w_double_tap_duration
    chown system system /sys/android_touch/s2w_double_tap_threshold
    chmod 0666 /sys/android_touch/s2w_double_tap_threshold
    chown system system /sys/android_touch/s2w_min_distance
    chmod 0666 /sys/android_touch/s2w_min_distance
    chown system system /sys/android_touch/s2w_register_threshold
    chmod 0666 /sys/android_touch/s2w_register_threshold
    chown system system /sys/android_touch/sweep2wake
    chmod 0666 /sys/android_touch/sweep2wake
    chown system system /sys/class/leds/button-backlight/slow_blink
    chmod 0666 /sys/class/leds/button-backlight/slow_blink

# Interactive governor settings
    chown system system /sys/devices/system/cpu/cpufreq/interactive/boost_factor
    chmod 0664 /sys/devices/system/cpu/cpufreq/interactive/boost_factor
    chown system system /sys/devices/system/cpu/cpufreq/interactive/go_maxspeed_load
    chmod 0664 /sys/devices/system/cpu/cpufreq/interactive/go_maxspeed_load
    chown system system /sys/devices/system/cpu/cpufreq/interactive/io_is_busy
    chmod 0664 /sys/devices/system/cpu/cpufreq/interactive/io_is_busy
    chown system system /sys/devices/system/cpu/cpufreq/interactive/max_boost
    chmod 0664 /sys/devices/system/cpu/cpufreq/interactive/max_boost
    chown system system /sys/devices/system/cpu/cpufreq/interactive/sustain_load
    chmod 0664 /sys/devices/system/cpu/cpufreq/interactive/sustain_load

	#smartmax governor
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/up_rate
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/up_rate
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/down_rate
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/down_rate
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/suspend_ideal_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/suspend_ideal_freq
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/awake_ideal_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/awake_ideal_freq
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/ideal_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/ideal_freq
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/ramp_up_step
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/ramp_up_step
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/ramp_down_step
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/ramp_down_step
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/max_cpu_load
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/max_cpu_load
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/min_cpu_load
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/min_cpu_load
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/sampling_rate
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/sampling_rate
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/input_boost_duration
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/input_boost_duration
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/io_is_busy
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/io_is_busy
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/ignore_nice
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/ignore_nice
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/touch_poke_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/touch_poke_freq
    chown system system /sys/devices/system/cpu/cpufreq/smartmax/boost_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax/boost_freq

	#smartmax_eps governor
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/up_rate
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/up_rate
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/down_rate
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/down_rate
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/suspend_ideal_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/suspend_ideal_freq
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/awake_ideal_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/awake_ideal_freq
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/ideal_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/ideal_freq
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/ramp_up_step
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/ramp_up_step
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/ramp_down_step
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/ramp_down_step
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/max_cpu_load
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/max_cpu_load
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/min_cpu_load
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/min_cpu_load
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/sampling_rate
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/sampling_rate
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/input_boost_duration
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/input_boost_duration
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/io_is_busy
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/io_is_busy
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/ignore_nice
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/ignore_nice
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/touch_poke_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/touch_poke_freq
    chown system system /sys/devices/system/cpu/cpufreq/smartmax_eps/boost_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/smartmax_eps/boost_freq

	#intelliactive governor
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/target_loads
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/target_loads
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/above_hispeed_delay
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/above_hispeed_delay
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/hispeed_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/hispeed_freq
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/go_hispeed_load
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/go_hispeed_load
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/min_sample_time
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/min_sample_time
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/timer_rate
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/timer_rate
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/timer_slack
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/timer_slack
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/boost
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/boost
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/boostpulse
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/boostpulse
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/boostpulse_duration
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/boostpulse_duration
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/io_is_busy
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/io_is_busy
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/sampling_down_factor
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/sampling_down_factor
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/sync_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/sync_freq
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/up_threshold_any_cpu_load
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/up_threshold_any_cpu_load
    chown system system /sys/devices/system/cpu/cpufreq/intelliactive/up_threshold_any_cpu_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/up_threshold_any_cpu_freq
	chown system system /sys/devices/system/cpu/cpufreq/intelliactive/two_phase_freq
    chmod 0660 /sys/devices/system/cpu/cpufreq/intelliactive/two_phase_freq

# liblights
    chown system system /sys/class/leds/button-backlight/currents
    chmod 0664 /sys/class/leds/button-backlight/currents
    chown system system /sys/class/leds/button-backlight/button_brightness
    chmod 0664 /sys/class/leds/button-backlight/button_brightness

# a2dp freq boost
    chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq
    chmod 0664 /sys/devices/platform/tegra_uart.2/a2dp_tuning

# permission for torch app
    chmod 0666 /sys/class/leds/flashlight/brightness

# Mediaserver: camera calibration
    chown system system /dev/block/mmcblk0p2
    chmod 0664 /dev/block/mmcblk0p2

on boot
    # bluetooth
    exec /system/bin/logwrapper /system/bin/load-bt.sh
    insmod /system/lib/modules/uhid.ko

# Replaces HTC's upd_nvs_wlan0 solution with our own
service wifi_calib /system/bin/wifi_calibration.sh
    disabled
    class wifi
    user root
    group root
    oneshot
